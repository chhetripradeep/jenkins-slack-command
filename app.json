{
  "name": "Jenkins Slack Command Sample",
  "description": "An instance for Jenkins Slack Command for starting builds",
  "repository": "https://github.com/joshdholtz/jenkins-slack-command",
  "keywords": ["jenkins", "slack", "command", "awesome"],
  "env": {
    "SLACK_TOKEN": {
      "description": "Token from your 'Slash Command'",
      "required": true
    },
    "SLACK_WEBHOOK_URL": {
      "description": "Incoming webhook URL"
    },
    "JENKINS_URL": {
      "description": "Jenkins instnace URL",
      "required": true
    },
    "JENKINS_TOKEN": {
      "description": "API token of Jenkins",
      "required": true
    }
  }
}
